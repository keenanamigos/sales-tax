{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "align": [true, "parameters", "arguments", "statements"],
        "class-name": true,
        "comment-format": [true, "check-space", "check-uppercase"],
        "curly": true,
        "interface-name": [true, "always-prefix"],
        "indent": [true, "tabs"],
        "new-parens": true,
        "no-any": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-empty": true,
        "no-empty-interface": true,
        "no-eval": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unreachable": true,
        "no-var-keyword": true,
        "member-access": [true, "check-accessor", "check-constructor"],
        "one-line": [true, "check-catch", "check-else", "check-open-brance", "check-whitespace"],
        "one-variable-per-declaration": [true, "ignore-for-loop"],
        "quotemark": [true, "double"],
        "radix": true,
        "semicolon": [true, "always"],
        "triple-equals": true,
        "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
        "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module", "check-type"]
    },
    "rulesDirectory": []
}